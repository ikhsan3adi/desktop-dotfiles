{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "rosewater": "#f2d5cf",
    "flamingo": "#eebebe",
    "pink": "#f4b8e4",
    "mauve": "#ca9ee6",
    "red": "#e78284",
    "maroon": "#ea999c",
    "peach": "#ef9f76",
    "yellow": "#e5c890",
    "green": "#a6d189",
    "teal": "#81c8be",
    "sky": "#99d1db",
    "sapphire": "#85c1dc",
    "blue": "#8caaee",
    "lavender": "#babbf1",
    "text": "#c6d0f5",
    "subtext1": "#b5bfe2",
    "subtext0": "#a5adce",
    "overlay2": "#949cbb",
    "overlay1": "#838ba7",
    "overlay0": "#737994",
    "surface2": "#626880",
    "surface1": "#51576d",
    "surface0": "#414559",
    "base": "#303446",
    "mantle": "#292c3c",
    "crust": "#232634",
    "accent": "p:teal"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:accent",
          "foreground": "p:base",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<background,p:base>\ue0b0</>",
          "style": "diamond",
          "type": "text",
          "template": "â€‰<b>{{ if ne .Code 0 }}!{{ else }}{{if .Root }}#{{ else }}${{ end }}{{ end }}</b>â€‰",
          "background_templates": [
            "{{ if ne .Code 0 }}p:red{{ end }}"
          ],
          "foreground_templates": [
            "{{ if ne .Code 0 }}p:base{{ end }}"
          ]
        },
        {
          "background": "p:surface0",
          "foreground": "p:text",
          "style": "diamond",
          "leading_diamond": "<p:base,background>\ue0b0</>",
          "trailing_diamond": "<background,p:base>\ue0b0</>",
          "template": " {{if .Root }}<b><p:red>{{ .UserName }}</></b>{{else}}{{ .UserName }}{{end}}@<b><p:accent>{{ .HostName }}</></b> ",
          "type": "session"
        },
        {
          "background": "p:surface1",
          "foreground": "p:text",
          "style": "diamond",
          "leading_diamond": "<p:base,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "folder_icon": "...",
            "folder_separator_icon": "/",
            "right_format": "<b><i><p:accent>%s</></i></b>",
            "home_icon": "~",
            "style": "powerlevel",
            "max_width": 32
          },
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "git",
          "background": "p:surface2",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:text{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:green{{ end }}",
            "{{ if gt .Behind 0 }}p:yellow{{ end }}"
          ],
          "template": " <b>{{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }} {{ end }}{{ if .Working.Changed }}ðŸ–‰ {{ .Working.String }} {{ end }}{{ if .Staging.Changed }}ðŸ—¹ {{ .Staging.String }} {{ end }}{{ if gt .StashCount 0 }}ðŸ–« {{ .StashCount }} {{ end }}</b>",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "source": "cli",
            "branch_icon": "\uE0A0 ",
            "branch_identical_icon": "\u2261",
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone_icon": "\u2262",
            "commit_icon": "\uF417 ",
            "tag_icon": "\uF412 ",
            "rebase_icon": "\uE728 ",
            "cherry_pick_icon": "\uE29B ",
            "revert_icon": "\uF0E2 ",
            "merge_icon": "\uE727 ",
            "no_commits_icon": "\uF594 "
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}